{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/samerfarida/mcp-ssh-orchestrator/wiki"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "^mailto:"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://github.com/samerfarida/mcp-ssh-orchestrator/wiki/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept-Encoding": "gzip, deflate, br"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3
}
